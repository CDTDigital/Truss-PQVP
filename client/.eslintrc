{
    parser: "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "react/forbid-prop-types": [2, {"forbid": ['any']}],
        "react/jsx-filename-extension": 0,
        "no-undef": 0,
        "global-require": 0,
        "no-console": 0
    }
}
